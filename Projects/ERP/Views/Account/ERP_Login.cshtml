    <style>
        .login-page {
            padding-top: 100px;
        }

        a.logo {
            display: block;
            text-align: center;
        }

            a.logo img {
                margin: 0 auto 15px;
            }

        .form-holder .form-control {
            border-radius: 30px;
        }
    </style>

</head>

<body>
    <div class="wrapper login-page">
        <header class="" style="padding: 10px;"></header>
        <section class="Content">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-md-offset-3">
                        <div class="logo-container">
                            <a href="" title="الصرح - نظام تخطيط موارد المؤسسات ونظام الاملاك والعقود" class="logo">
                                <img style="height:150px;" src="/ERPTest/Content/Theme/DefaultTheme/images/asarh.png"
                                     alt="الصرح - نظام تخطيط موارد المؤسسات ونظام الاملاك والعقود" class="img-responsive">
                            </a>
                        </div>
                        <!-- end - logo-container -->
                        <div class="form-holder">
                            <div>
                                <form action="/ERPTest/Account/LogOn/frmManager?name=frmManager&amp;controlPanel=True" method="post">

                            </div>
                            <div class="form-group">
                                <input autocomplete="off" class="form-control" data-val="true" data-val-required="يجب ادخال اسم المستخدم" id="Username" name="Username" placeholder="اسم المستخدم" type="text" value="" />
                                <span class="field-validation-valid" data-valmsg-for="Username" data-valmsg-replace="true"></span>
                            </div>
                            <div class="form-group">
                                <input class="form-control" data-val="true" data-val-required="يجب ادخال كلمة المرور" id="Password" name="Password" placeholder="كلمة المرور" type="password" />
                                <span class="field-validation-valid" data-valmsg-for="Password" data-valmsg-replace="true"></span>
                            </div>
                            <div class="checkbox" style="display:none;">
                                <label><input type="checkbox" name="remember"> Remember me</label>
                            </div>
                            <button type="submit" class="btn custom-btn btn-block">دخول</button>
                        </div>
                    </div>

                </div>
                <!-- end - row -->
            </div>
            <!-- end - container -->
        </section>
        <!-- end - Content -->
        <footer class="login">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="copyright">
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- end - footer -->
